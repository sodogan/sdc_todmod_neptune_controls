				<splash src="res/screens/android/splash-port-xxxhdpi.png" density="xxxhdpi"/>
				<icon src="res/icons/android/icon-hdpi.png" density="hdpi"/>
				<icon src="res/icons/android/icon-ldpi.png" density="ldpi"/>
				<icon src="res/icons/android/icon-mdpi.png" density="mdpi"/>
				<icon src="res/icons/android/icon-xhdpi.png" density="xhdpi"/>
				<icon src="res/icons/android/icon-xxhdpi.png" density="xxhdpi"/>
				<icon src="res/icons/android/icon-xxxhdpi.png" density="xxxhdpi"/>
				<allow-intent href="market:*"/>
				<edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application">
						<application allowBackup="false" android:usesCleartextTraffic="true"/>

				</edit-config>

		</platform>
		<platform name="ios">
				<preference name="SurpressLongPressGesture" value="true"/>
				<edit-config target="NSCameraUsageDescription" file="*-Info.plist" mode="merge">
						<string>to take pictures/scan barcodes</string>

				</edit-config>
				<edit-config target="NSPhotoLibraryUsageDescription" file="*-Info.plist" mode="merge">
						<string>to get pictures</string>

				</edit-config>
				<edit-config target="NSLocationWhenInUseUsageDescription" file="*-Info.plist" mode="merge">
						<string>to find things nearby</string>

				</edit-config>
				<edit-config target="NSFaceIDUsageDescription" file="*-Info.plist" mode="merge">
						<string>to log on</string>

				</edit-config>
				<edit-config target="NSMicrophoneUsageDescription" file="*-Info.plist" mode="merge">
						<string>capture video audio</string>

				</edit-config>
